{
    "name": "risecli",
    "version": "0.0.50",
    "bin": {
        "rise": "./bin/run"
    },
    "bugs": "https://github.com/rise-cli/risecli/issues",
    "scripts": {
        "postpack": "rm -f oclif.manifest.json",
        "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
        "ci:test": "CI=true ./node_modules/.bin/jest",
        "local:test": "./node_modules/.bin/jest --coverage",
        "dev:test": "jest --watch --maxWorkers=1 $1",
        "version": "oclif-dev readme && git add README.md"
    },
    "dependencies": {
        "@graphql-tools/code-file-loader": "^6.3.1",
        "@graphql-tools/load": "^6.2.8",
        "@graphql-tools/merge": "^6.2.14",
        "@oclif/command": "^1",
        "@oclif/config": "^1",
        "@oclif/plugin-help": "^3",
        "aws-sdk": "^2.939.0",
        "chalk": "^4.1.1",
        "cli-ux": "^5.6.2",
        "graphql": "^15.5.1",
        "graphql-tag": "^2.12.5",
        "json-to-pretty-yaml": "^1.2.2",
        "tslib": "^1"
    },
    "devDependencies": {
        "@oclif/dev-cli": "^1",
        "@types/jest": "^26.0.24",
        "@types/node": "^10",
        "globby": "^10",
        "jest": "^27.0.6",
        "ts-jest": "^27.0.4",
        "ts-node": "^8",
        "typescript": "^3.3"
    },
    "engines": {
        "node": ">=8.0.0"
    },
    "files": [
        "/bin",
        "/lib",
        "/npm-shrinkwrap.json",
        "/oclif.manifest.json"
    ],
    "homepage": "https://github.com/rise-cli/risecli",
    "keywords": [
        "oclif"
    ],
    "license": "MIT",
    "main": "lib/index.js",
    "oclif": {
        "commands": "./lib/commands",
        "bin": "rise",
        "plugins": [
            "@oclif/plugin-help"
        ]
    },
    "repository": "rise-cli/risecli",
    "types": "lib/index.d.ts"
}
